<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
    <ImportAction name="merge">
        <Configuration name="PluginFrameworkConfig">
            <Attributes>
                <Map>
                    <entry key="Plugins">
                        <value>
                            <Map>
                                <entry key="customConnector">
                                    <value>
                                        <Plugin uniqueName="customConnector" displayName="Custom Connector" enabled="true"
                                            version="0.0.2" allowDisable="true" visible="true" allowUninstall="true"
                                            minFrameworkVersion="0.1.0" maxFrameworkVersion="1.0.0"
                                            minUpgradableVersion="0.1.3" certificationLevel="BASIC"
                                            pluginAccessRight="customConnectorPluginRight">

                                            <PluginConfiguration
                                                settingsPageTemplateURL="ui/htmlTemplates/pluginSettings.html">
                                                <FullPage desktopIncludePath="{plugin_root}/ui/mainConnection.html"
                                                  mobileIncludePath="{plugin_root}/ui/mainConnection.html"/>
                                                  <Snippets>
                                                          <Snippet pagesMatchRegExPattern=".*" isMobile="true" isDesktop="true">
                                                              <ScriptIncludes>
                                                                  <String>{plugin_root}/ui/js/inject.js</String>
                                                              </ScriptIncludes>
                                                          </Snippet>
                                                  </Snippets>
                                              </PluginConfiguration>
                                              <ConfigurableSettings>
                                                <Map>
                                                    <entry key="Message" value="Custom Connector"/>
                                                </Map>
                                            </ConfigurableSettings>
                                        </Plugin>
                                    </value>
                                </entry>
                            </Map>
                        </value>
                    </entry>
                </Map>
            </Attributes>
        </Configuration>
    </ImportAction>
</sailpoint>



